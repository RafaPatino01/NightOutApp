{
  "expo": {
    "name": "NightOut",
    "slug": "NightOut",
    "version": "1.0.17",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "scheme": "nightout",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.nightoutmx.nightoutmx",
      "runtimeVersion": "1.0.17",
      "buildNumber": "17",
      "jsEngine": "jsc",
      "associatedDomains": ["applinks:nightout.com.mx"]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "runtimeVersion": "1.0.17",
      "package": "com.nightoutmx.nightoutmx",
      "versionCode": 8,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "https",
            "host": "nightout.com.mx",
            "pathPrefix": "/path"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b02177bc-1fb2-4387-b19e-2baf27338d63"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/b02177bc-1fb2-4387-b19e-2baf27338d63"
    }
  }
}
